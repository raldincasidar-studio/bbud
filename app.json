{
  "expo": {
    "name": "BBud",
    "slug": "bbud",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.raldincasidar.bbud"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.raldincasidar.bbud"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            {
              "src": "./assets/fonts/Poppins-Regular.ttf",
              "fontFamily": "Poppins-Regular"
            },
            {
              "src": "./assets/fonts/Poppins-Bold.ttf",
              "fontFamily": "Poppins-Bold"
            }
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "35009995-0f19-4b12-bcb7-06ed38ca419c"
      }
    },
    "owner": "raldincasidar",
    "updates": {
      "url": "https://u.expo.dev/35009995-0f19-4b12-bcb7-06ed38ca419c"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
